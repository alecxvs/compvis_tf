    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "program": "${file}"
        },
        {
            "name": "Python cProfile: Current File",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "module": "cProfile",
            "args": [
                "-s",
                "time",
                "-o",
                "run.profile",
                "${file}"
            ]
        }
    ]
}